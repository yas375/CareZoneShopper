PODS_ROOT = ${SRCROOT}/Pods
PODS_HEADERS_SEARCH_PATHS = ${PODS_PUBLIC_HEADERS_SEARCH_PATHS}
ALWAYS_SEARCH_USER_PATHS = YES
OTHER_LDFLAGS = -ObjC -lxml2 -framework CFNetwork -framework CoreData -framework CoreGraphics -framework Security -framework SystemConfiguration
HEADER_SEARCH_PATHS = $(SDKROOT)/usr/include/libxml2 ${PODS_HEADERS_SEARCH_PATHS}
PODS_BUILD_HEADERS_SEARCH_PATHS = "${PODS_ROOT}/BuildHeaders" "${PODS_ROOT}/BuildHeaders/KissXML" "${PODS_ROOT}/BuildHeaders/MBProgressHUD" "${PODS_ROOT}/BuildHeaders/MKFoundation" "${PODS_ROOT}/BuildHeaders/MKNetworkKit" "${PODS_ROOT}/BuildHeaders/MKNetworkKit/Categories" "${PODS_ROOT}/BuildHeaders/MagicalRecord" "${PODS_ROOT}/BuildHeaders/Reachability" "${PODS_ROOT}/BuildHeaders/SSPullToRefresh"
PODS_PUBLIC_HEADERS_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/KissXML" "${PODS_ROOT}/Headers/MBProgressHUD" "${PODS_ROOT}/Headers/MKFoundation" "${PODS_ROOT}/Headers/MKNetworkKit" "${PODS_ROOT}/Headers/MKNetworkKit/Categories" "${PODS_ROOT}/Headers/MagicalRecord" "${PODS_ROOT}/Headers/Reachability" "${PODS_ROOT}/Headers/SSPullToRefresh"